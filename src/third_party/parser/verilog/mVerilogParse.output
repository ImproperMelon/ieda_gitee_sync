Grammar

    0 $accept: file $end

    1 file: modules

    2 modules: %empty
    3        | modules module

    4 @1: %empty

    5 module_begin: MODULE @1

    6 module: module_begin ID ';' stmts ENDMODULE
    7       | module_begin ID '(' ')' ';' stmts ENDMODULE
    8       | module_begin ID '(' port_list ')' ';' stmts ENDMODULE
    9       | module_begin ID '(' port_dcls ')' ';' stmts ENDMODULE
   10       | module_begin ID '#' '(' module_parameters ')' '(' port_dcls ')' ';' stmts ENDMODULE

   11 port_list: port
   12          | port_list ',' port

   13 port: port_expr
   14     | '.' ID '(' ')'
   15     | '.' ID '(' port_expr ')'

   16 port_expr: port_ref
   17          | '{' port_refs '}'

   18 port_refs: port_ref
   19          | port_refs ',' port_ref

   20 port_ref: net_scalar
   21         | net_bit_select
   22         | net_part_select

   23 port_dcls: port_dcl
   24          | port_dcls ',' port_dcl
   25          | port_dcls ',' dcl_arg

   26 @2: %empty

   27 port_dcl: port_dcl_type @2 dcl_arg

   28 @3: %empty

   29 port_dcl: port_dcl_type @3 '[' parameter_expr ':' parameter_expr ']' dcl_arg

   30 port_dcl_type: INPUT
   31              | INPUT WIRE
   32              | INOUT
   33              | INOUT REG
   34              | INOUT WIRE
   35              | OUTPUT
   36              | OUTPUT WIRE
   37              | OUTPUT REG

   38 stmts: %empty
   39      | stmts stmt
   40      | stmts stmt_seq

   41 stmt: parameter
   42     | defparam
   43     | declaration
   44     | instance
   45     | error ';'

   46 stmt_seq: continuous_assign

   47 parameter: PARAMETER parameter_dcls ';'
   48          | PARAMETER '[' INT ':' INT ']' parameter_dcls ';'

   49 module_parameters: module_parameter
   50                  | module_parameters ',' module_parameter

   51 module_parameter: PARAMETER parameter_dcl
   52                 | PARAMETER '[' INT ':' INT ']' parameter_dcl

   53 parameter_dcls: parameter_dcl
   54               | parameter_dcls ',' parameter_dcl

   55 parameter_dcl: ID '=' parameter_expr
   56              | ID '=' STRING

   57 parameter_expr: ID
   58               | '`' ID
   59               | CONSTANT
   60               | INT
   61               | '-' parameter_expr
   62               | parameter_expr '+' parameter_expr
   63               | parameter_expr '-' parameter_expr
   64               | parameter_expr '*' parameter_expr
   65               | parameter_expr '/' parameter_expr
   66               | '(' parameter_expr ')'
   67               | '.' parameter_expr '(' parameter_expr ')'

   68 defparam: DEFPARAM param_values ';'

   69 param_values: param_value
   70             | param_values ',' param_value

   71 param_value: ID '=' parameter_expr
   72            | ID '=' STRING

   73 @4: %empty

   74 declaration: dcl_type @4 dcl_args ';'

   75 @5: %empty

   76 declaration: dcl_type @5 '[' INT ':' INT ']' dcl_args ';'

   77 dcl_type: INPUT
   78         | INOUT
   79         | OUTPUT
   80         | SUPPLY0
   81         | SUPPLY1
   82         | TRI
   83         | WAND
   84         | WIRE
   85         | WOR

   86 dcl_args: dcl_arg
   87         | dcl_args ',' dcl_arg

   88 dcl_arg: ID
   89        | net_assignment

   90 continuous_assign: ASSIGN net_assignments ';'

   91 net_assignments: net_assignment
   92                | net_assignments ',' net_assignment

   93 @6: %empty

   94 net_assignment: net_assign_lhs @6 '=' net_expr

   95 net_assign_lhs: net_named
   96               | net_expr_concat

   97 @7: %empty

   98 instance: ID @7 ID '(' inst_pins ')' ';'

   99 @8: %empty

  100 instance: ID @8 parameter_values ID '(' inst_pins ')' ';'

  101 parameter_values: '#' '(' parameter_exprs ')'

  102 parameter_exprs: parameter_expr
  103                | '{' parameter_exprs '}'
  104                | parameter_exprs ',' parameter_expr

  105 inst_pins: %empty
  106          | inst_ordered_pins
  107          | inst_named_pins

  108 inst_ordered_pins: net_expr
  109                  | inst_ordered_pins ',' net_expr

  110 inst_named_pins: inst_named_pin
  111                | inst_named_pins ',' inst_named_pin

  112 inst_named_pin: '.' ID '(' ')'
  113               | '.' ID '(' ID ')'
  114               | '.' ID '(' ID '[' INT ']' ')'
  115               | '.' ID '(' named_pin_net_expr ')'
  116               | '.' ID '[' INT ']' '(' ')'
  117               | '.' ID '[' INT ']' '(' net_expr ')'
  118               | '.' ID '[' INT ':' INT ']' '(' ')'
  119               | '.' ID '[' INT ':' INT ']' '(' net_expr ')'

  120 named_pin_net_expr: net_part_select
  121                   | net_constant
  122                   | net_expr_concat

  123 net_named: net_scalar
  124          | net_bit_select
  125          | net_part_select

  126 net_scalar: ID

  127 net_bit_select: ID '[' INT ']'

  128 net_part_select: ID '[' INT ':' INT ']'

  129 net_constant: CONSTANT

  130 net_expr_concat: '{' net_exprs '}'

  131 net_exprs: net_expr
  132          | net_exprs ',' net_expr

  133 net_expr: net_scalar
  134         | net_bit_select
  135         | net_part_select
  136         | net_constant
  137         | net_expr_concat


